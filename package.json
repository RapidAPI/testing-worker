{
  "name": "@rapidapi/testing-worker",
  "version": "1.0.8",
  "description": "A RapidAPI Testing worker CLI",
  "private": false,
  "license": "MIT",
  "scripts": {
    "worker:local:interval": "node --unhandled-rejections=strict src/main.js",
    "lint": "yarn eslint src/**",
    "format": "yarn prettier -w src/**",
    "check-format": "yarn prettier -c src/**",
    "test": "yarn jest --watch"
  },
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:RapidAPI/testing-worker.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Lucas Swartsenburg",
  "homepage": "https://github.com/RapidAPI/testing-worker#readme",
  "dependencies": {
    "@sentry/node": "^6.2.5",
    "@sentry/serverless": "^5.29.2",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "axios": "^0.21.1",
    "axios-cookiejar-support": "^1.0.1",
    "chalk": "^4.1.0",
    "commander": "^8.1.0",
    "consola": "^2.15.0",
    "faker": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "mssql": "^6.3.1",
    "mustache": "^4.1.0",
    "mysql": "^2.18.1",
    "pg": "^8.6.0",
    "tedious": "^11.0.8",
    "tough-cookie": "^4.0.0",
    "uuid": "^8.3.2",
    "vm2": "^3.9.5",
    "xml-js": "^1.6.11"
  },
  "bin": "src/main.js",
  "devDependencies": {
    "@actions/core": "^1.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "^27.0.6",
    "prettier": "2.2.1",
    "sinon": "^11.1.2",
    "yarn": "^1.22.11"
  }
}
